{
    "name": "juststeveking/invoice-number",
    "description": "A simple to use PHP Invoice Number Generator.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "role": "Developer",
            "name": "Steve McDougall",
            "email": "juststevemcd@gmail.com",
            "homepage": "https://www.juststeveking.uk/"
        }
    ],
    "autoload": {
        "psr-4": {
            "JustSteveKing\\InvoiceNumber\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JustSteveKing\\InvoiceNumber\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "hashids/hashids": "^5.0"
    },
    "require-dev": {
        "laravel/pint": "^1.5",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^1.22",
        "phpstan/phpstan": "^1.9",
        "symfony/var-dumper": "^6.2"
    },
    "scripts": {
        "pint": [
            "./vendor/bin/pint"
        ],
        "stan": [
            "./vendor/bin/phpstan analyse"
        ],
        "test": [
            "./vendor/bin/pest --coverage"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "JustSteveKing\\InvoiceNumber\\Providers\\PackageServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
